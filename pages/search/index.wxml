<view class="search_row">
    <input value="{{iptValue}}" placeholder="请输入您要搜索的数据" bindinput="handleInput"></input>
    <button size="mini" hidden="{{!isFocus}}" bindtap="handleCancel">
        取消
    </button>
</view>
<view class="search_content">
    <navigator 
    url="/pages/goods_detail/index?goods_id={{item.goods_id}}"
    class="search_item"
    wx:for="{{goods}}"
    wx:key="goods_id"
    >
    {{item.goods_name}}
    </navigator>
</view>